@model HebrewLanguageLearning_Admin.DBContext.HLL_Grammar
@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="page-content-inner ">
    <br>
    <h2>Add Grammar Topic</h2>
    @using (Ajax.BeginForm("Create", "Grammar", new AjaxOptions { UpdateTargetId = "result", OnBegin = "return StartValidation();" }, new { enctype = "multipart/form-data", @id = "CreateGrammar" }))
    {
        @*@using (Html.BeginForm()){*@
        @Html.AntiForgeryToken()
        <Div action="#" role="form">
            <div class="panel panel-default">
                <div class="panel-heading"><h6 class="panel-title"><i class="icon-pencil3"></i>Add Grammar</h6></div>
                <div class="panel-body">
                    <div class="text-danger" id="result"></div>
                    <div class="form-group">
                        <div class="row">
                            <div class="col-md-5">
                                <label>English Grammar Topic:</label>
                                @Html.EditorFor(model => model.EnglishGrammar, new { htmlAttributes = new { @id = "EnglishGrammar", @class = "form-control", @placeholder = "English Grammar" } })
                                @Html.ValidationMessageFor(x => x.EnglishGrammar, "", new { @class = "text_danger", @Style = "color: #D65C4F" })
                                <label id="EnglishGrammarErrorMessage" class="text-danger" style="display:none;color:red;margin-top: 30px;">Please English Grammar </label>

                            </div>

                            <div class="col-md-5">
                                <label>Hebrew Grammar Topic:</label>
                                @Html.EditorFor(model => model.HebrewGrammar, new { htmlAttributes = new { @id = "HebrewGrammar", @class = "form-control", @placeholder = "Hebrew Grammar" } })
                                @Html.ValidationMessageFor(x => x.HebrewGrammar, "", new { @class = "text_danger", @Style = "color: #D65C4F" })
                                <label id="HebrewGrammarErrorMessage" class="text-danger" style="display:none;color:red;margin-top: 30px;">Please Hebrew Grammar</label>

                            </div>
                            <div class="col-md-2">
                                <label>Import Excel File:</label>

                                <div class="controls imgParent">
                                    @Html.HiddenFor(h => h.GrammarId, new { name = "GrammarId", id = "GrammarId" })
                                    @Html.TextBoxFor(p => p.GrammarId, null, new { data_val = "false", type = "file", @class = "input-file", name = "Soundfile", id = "Soundfile" })
                                    <span class="fileupload"><span class="icon-upload2"></span>Choose File</span>
                                    @*@Html.ValidationMessageFor(n => n.Soundfile, null, new { @class = "text_danger", @style = "color: red !important;" })
                                        <div class="col-md-2" id="output"></div>
                                        <label id="fileUploaderErrorMessage" class="text-danger" style="display:none;color:red;margin-top: 30px;">Please Upload Sound file in MP3 format</label>*@

                                </div>

                            </div>
                        </div>
                    </div>

                    <hr>
                    <div id="Application_Data">
                        @{Html.RenderPartial("_HLL_Grammar_Data_PartialView", Model);}
                    </div>

                </div>
                <hr>
                <hr>
                <div class="form-group panel-body">
                    <div class="form-actions text-right">
                        <input type="reset" value="Cancel" class="btn btn-danger">
                        <input type="button" onclick="SubmitButtonOnclick()" value="Save and Continue" class="btn btn-primary">
                    </div>
                </div>
            </div>
        </Div>
                            }

</div>
<div>
    @Html.ActionLink("Back to List", "Index")
</div>
<!-- Form modal -->
<div id="ModalDiv"></div>
<!-- /form modal -->
@*<script src="https://code.jquery.com/jquery-2.2.4.min.js" integrity="sha256-BbhdlvQf/xTY9gja0Dq3HiwQF8LaCRTXxZKRutelT44=" crossorigin="anonymous"></script>*@

<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="~/Scripts/jquery.validate.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
<script src="~/Scripts/jquery.unobtrusive-ajax.js"></script>



<script type="text/javascript">
    $(document).ready(function () {

        var url = "";
        if (!url) url = window.location.href;
        var pathname = new URL(url).pathname;
        var SplitData = pathname.split('/');
        var GrammarId = SplitData[SplitData.length - 1];
        if (typeof GrammarId != 'undefined' && GrammarId.indexOf('HLLUSA001') > -1) {
            var _that = $('#GrammarId');
            $('.icon - pencil3').val("Edit Application");
            _that.val(GrammarId);
            _that.attr('disabled', 'disabled');
            getDetail(GrammarId, '../GetDataForm');
        }
        else { }


    });

    var textboxId = 5; var textboxName = 4;

    function StartValidation() {
        var AppSentenceDynamicTextBoxData = $('#AppSentenceDynamicTextBox').val(); $('#AppSentenceDynamicTextBoxErrorMessage').hide();
        if (typeof AppSentenceDynamicTextBoxData == 'undefined' || AppSentenceDynamicTextBoxData == "") {
            $('#AppSentenceDynamicTextBoxErrorMessage').show();
            return false;
        }

        else
            $('#fileUploaderErrorMessage').hide();

        return true;
    }
    function getFileExtension(filename) {
        if (typeof filename != 'undefined') {
            return filename.slice((filename.lastIndexOf(".") - 1 >>> 0) + 2);
        }
        return "";

    }
    function GetDynamicTextBox(value) {
        var div = $("<div class='row'><div class='col-md-4'>" +
                           "<label>Hebrew Exercise-" + textboxId + ":</label>" +
                           "<input class='form-control text-box single-line' id='AppSentenceDynamicTextBox' name='AppSentenceDynamicTextBox[" + textboxName + "]' placeholder='Hebrew Exercise - " + textboxId + "' type='text' value=''>" +
                           "<span class='field-validation-valid text-danger' data-valmsg-for='AppSentenceDynamicTextBox[0]' data-valmsg-replace='true'></span>" +
                           "<label id='AppSentenceDynamicTextBoxErrorMessage' class='text-danger' style='display:none;'>Please Enter At least one definition</label>    </div>" +
                           "<div class='col-md-4'><label>Picture/Video " + textboxId + ":</label><div class='controls imgParent'>" +
                           "<input id='SoundUrl' name='ApplicationId' type='hidden' value=''>" +
                           "<input class='input-file fileUploadImgAndVideo' data-val='false' id='" + textboxId + "' name='ApplicationId' type='file' value=''>" +
                           "<span class='fileupload'><span class='icon-upload2'></span>Choose File</span></div>   </div>" +
                           "<div class='col-md-4'><label>Sound " + textboxId + ":</label>" +
                           "<div class='controls imgParent'>" +
                           "<input id='SoundUrl' name='ApplicationId' type='hidden' value=''>" +
                           "<input class='input-file' data-val='false' id='Soundfile' name='ApplicationId' type='file' value=''>" +
                           "<span class='fileupload'><span class='icon-upload2'></span>Choose File</span>  </div></div> ");
                          
        textboxName++;
        textboxId++;
        return div;
    }
    function AddTextBox() {
        if (textboxId <= 12) {
            var div = GetDynamicTextBox("");
            $("#TextBoxContainer").append(div);
        }
    }
    $(function () {
        var values = eval('@Html.Raw(ViewBag.Values)');
        if (values != null) {
            $("#TextBoxContainer").html("");
            $(values).each(function () {
                $("#TextBoxContainer").append(GetDynamicTextBox(this));
            });
        }
    });
    var SetCorrectAnswer = function (CorrectAnswerID) {

        var ddlGrammarId = $('#GrammarId').val();
        if (isValid()) {
            $('#ModalDiv').html("<div id='form_modal_Definition' class='modal fade' tabindex='-1' role='dialog'></div>");
            $('#form_modal_Definition').load('@Url.Action("GetCorrectAnswerView", "Application")?CorrectAnswerNo=' + CorrectAnswerID + '&GrammarId=' + ddlGrammarId + '&isNext=false');
        }
    }
    var SaveFromDataCorrectAnswer = (function () {

        var CorrectAnswerID = $('#PopUp_CorrectAnswerID').val();
        var GrammarId = $('#GrammarId').val();
        $.ajax({
            type: "POST",
            url: '@Url.Action("CreateHebrewAppData", "Application")',
            data: $("#CreateHebrewAppData").serialize(),
            success: function (data) {
                window.location = '/Application/Create'
            }, err: function (data) { console.log("Server Is Not Respose" + data); }
        });
    });
    var SaveFromDataAndNext = (function () {
        var CorrectAnswerID = $('#PopUp_CorrectAnswerID').val();
        var GrammarId = $('#GrammarId').val();
        $.ajax({
            type: "POST",
            url: '@Url.Action("CreateHebrewAppData", "Application")',
            data: $("#CreateHebrewAppData").serialize(),
            success: function (data) {
                $('#form_modal_Definition').load('@Url.Action("GetCorrectAnswerView", "Application")?CorrectAnswerNo=' + CorrectAnswerID + '&GrammarId=' + GrammarId + '&isNext=true');
            }, err: function (data) {
                console.log("Server Is Not Respose" + data);
            }
        });

    });
    isValid = function () {
        var txtEnglishGrammarData = $('#EnglishGrammar').val(); var txtHebrewGrammarData = $('#HebrewGrammar').val();
        $('#EnglishGrammarErrorMessage').hide(); $('#HebrewGrammarErrorMessage').hide();
        if (typeof txtEnglishGrammarData == 'undefined' || txtEnglishGrammarData == "") {
            $('#EnglishGrammarErrorMessage').show();
            return false;
        }
        if (typeof txtHebrewGrammarData == 'undefined' || txtHebrewGrammarData == "") {
            $('#HebrewGrammarErrorMessage').show();
            return false;
        }
        return true;

    }
    var getDetail = function (x, baseUrl) {
        $.ajax({ url: baseUrl, method: 'post', data: { GrammarId: x, __RequestVerificationToken: document.getElementsByName('__RequestVerificationToken')[0].value } }).done(function (res) {
            $('#Application_Data').html(res);
            checker();
        });
    };
    var $containerCon = $('#Application_Data');

    var checker = function () {
        textboxArr = $containerCon.find('.single-line');
        textboxArr.each(function (i, v) {
            var _that = $(this);
            var parent = $(this).closest('.row');
            var videourl = parent.find('input[name="VideoUrl[' + i + ']"]');
            var soundurl = parent.find('input[name="SoundUrl[' + i + ']"]');
            setImagesOnApplicationDataView(i + 1, videourl.val());
            setImagesOnApplicationDataView(i + 1, soundurl.val());
        });
    };

    var setImagesOnApplicationDataView = function (txtId, imgValue) {
        var h = "";
        if (typeof imgValue != 'undefined') {
            switch (imgValue) {

                case "0":
                    var h = "<div class='imgcont'><img src='data:image/png;base64,' title = '' class='imageThumb'><i class='fa fa-minus del'></i></div>";
                    $("#outputMultiple" + txtId + "").html(h);
                    break;
                case "1":
                    h = "<div class='imgcont'><img src='data:image/png;base64,' title = '' class='imageThumb'><i class='fa fa-minus del'></i></div>";
                    $("#output" + txtId + "").html(h);
                    break;
                case "2":
                    h = "<div class='imgcont'><img src='data:image/png;base64,' title = '' class='imageThumb'><i class='fa fa-minus del'></i></div>";
                    $("#output" + txtId + "").html(h);
                    break;
                default:
                    //h = "No File";
                    $("#output" + txtId + "").html(h);
            }

        }

    }
    // 09-11-2017
    function SubmitButtonOnclick() {

        if (isValid()) {
            var formElement = document.querySelector("#CreateGrammar");
            var formData = new FormData(formElement);
            var xhr = new XMLHttpRequest();
            xhr.open("POST", "/Grammar/Create", true);
            xhr.addEventListener("load", function (evt) { UploadComplete(evt); }, false);
            xhr.addEventListener("error", function (evt) { UploadFailed(evt); }, false);
            xhr.send(formData);
        }
    }

    function UploadComplete(evt) {
        console.log("> " + JSON.stringify(evt));
        if (evt.target.status == 200) {
            window.location = '/Grammar/Index';
            // alert("Logo uploaded successfully.");
        }
        else {
            alert("Error Uploading File");
        }

    }

    function UploadFailed(evt) {
        alert("There was an error attempting to upload the file.");

    }

</script>
